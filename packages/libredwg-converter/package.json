{
  "name": "@mlightcad/libredwg-converter",
  "version": "3.0.9",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git://github.com/mlight-lee/realdwg-web"
  },
  "keywords": [
    "autocad",
    "cad",
    "dwg",
    "dxf",
    "libredwg",
    "libredwg-web",
    "mlight",
    "mlightcad",
    "objecarx",
    "realdwg"
  ],
  "files": [
    "dist",
    "lib",
    "README.md",
    "package.json"
  ],
  "main": "./dist/libredwg-converter.umd.cjs",
  "module": "./dist/libredwg-converter.js",
  "types": "./lib/index.d.ts",
  "exports": {
    "types": "./lib/index.d.ts",
    "import": "./dist/libredwg-converter.js",
    "require": "./dist/libredwg-converter.umd.cjs"
  },
  "scripts": {
    "clean": "rimraf dist lib tsconfig.tsbuildinfo",
    "build": "tsc && vite build -c vite.config.main.ts && vite build -c vite.config.worker.ts",
    "build:main": "vite build -c vite.config.main.ts",
    "build:worker": "vite build -c vite.config.worker.ts",
    "lint": "eslint src/",
    "lint:fix": "eslint --fix --quiet src/"
  },
  "devDependencies": {
    "vite": "^5.2.10"
  },
  "peerDependencies": {
    "@mlightcad/data-model": "workspace:*",
    "@mlightcad/libredwg-web": "0.2.1"
  }
}
